import kagglehub
import shutil
import os
import pandas as pd

path = kagglehub.dataset_download("faviovaz/bank-term-deposit")

destination_folder = "/content"

source_csv_file = os.path.join(path, "bank_term_deposit.csv")
destination_csv_file = os.path.join(destination_folder, "bank_term_deposit.csv")

df = pd.read_csv("/content/bank_term_deposit.csv", sep =";")
df

df_null = df.isnull().sum()
df_null

df.mean(axis = 0, skipna = True, numeric_only = True)

# แทนค่า null ด้วยค่าเฉลี่ย ในคอลัมน์ age
df["age"].fillna("41.6", inplace = True)
# แทนค่า null ด้วยค่าเฉลี่ย ในคอลัมน์ balance
df["balance"].fillna("1136.75", inplace = True)
df

df.isnull().sum()

# ดูประเภทของแต่ละคอลัมน์ว่าถูกต้องตาม format มั้ย
df.dtypes

# จะเห็นว่ามี 2 คอลัมน์ที่ผิด data type
df[['age', 'balance']].dtypes

# ทำการแปลงค่าให้เป็นตัวเลข
df['age'] = df['age'].astype(float)
df['balance'] = df['balance'].astype(float)

df.to_csv("/content/output.csv")
